{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/formulario-perfil-aluno" 
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/formulario-perfil-aluno/index.html"
      }
    ]
  }